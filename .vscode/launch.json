{
  "version": "0.2.0",
  "configurations": [{
      "type": "node",
      "request": "launch",
      "name": "启动投诉订单采集服务",
      "program": "${workspaceRoot}/robot-complaints-collector.js"
    }, {
      "type": "node",
      "request": "launch",
      "name": "启动投诉订单处理服务",
      "program": "${workspaceRoot}/robot-complaints-handler.js"
    }, {
      "type": "node",
      "request": "launch",
      "name": "启动发货订单监听服务",
      "program": "${workspaceRoot}/robot-exceptionOrder-listener.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "启动web应用",
      "program": "${workspaceRoot}/index.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "启动测试",
      "program": "${workspaceRoot}/test.js"
    },
    {
      "name": "Launch",
      "type": "phantomjs",
      "request": "launch",
      "file": "${workspaceRoot}/webpage.js",
      "webRoot": "${workspaceRoot}",
      "runtimeExecutable": "/usr/local/bin/phantomjs",
      "runtimeArgs": [],
      "sourceMaps": true
    }
  ]
}
