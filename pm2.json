{
  "apps": [{
    "name": "robot",
    "script": "index.js",
    "log_date_format": "YYYY-MM-DD HH",
    "error_file": "logs/robot-err.log",
    "pid_file": "logs/robot.pid",
    "exec_mode": "cluster",
    "instances": 1,
    "env": {
      "NODE_ENV": "development"
    },
    "env_test": {
      "NODE_ENV": "test"
    },
    "env_release": {
      "NODE_ENV": "production"
    }
  }]
}
